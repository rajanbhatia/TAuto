<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>
    <!-- report theme File Location: http://extentreports.com/docs/versions/2/java/#configuration-->
    <!-- standard, dark -->
    <theme>standard</theme>
  
    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>
    
    <!-- protocol for script and stylesheets -->
    <!-- defaults to https -->
    <protocol>https</protocol>
    
    <!-- title of the document -->
    <documentTitle>Automation Test Execution</documentTitle>
    
    <!-- report name - displayed at top-nav -->
    <reportName>Automation Testing Report:</reportName>
    
    <!-- report headline - displayed at top-nav, after reportHeadline -->
    <!-- >reportHeadline>UAT Environment</reportHeadline-->
    
    <!-- location of charts in the test view -->
    <!-- top, bottom -->
    <testViewChartLocation>top</testViewChartLocation>
    
    <!-- global date format override -->
    <!-- defaults to yyyy-MM-dd -->
    <dateFormat>dd-MM-yyyy</dateFormat>
    
    <!-- global time format override -->
    <!-- defaults to HH:mm:ss -->
    <timeFormat>HH:mm:ss</timeFormat>
    
    <!-- custom javascript -->
    <scripts>
      <![CDATA[
        $(document).ready(function() {
          
        });
      ]]>
    </scripts>
    
    <!-- custom styles -->
    <styles>
      <![CDATA[
        
      ]]>
    </styles>
  </configuration>
</extentreports>
